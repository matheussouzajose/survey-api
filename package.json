{
  "name": "surveys",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "dev": "sucrase-node src/main/server.ts",
    "up": "npm run build && docker compose up -d --build",
    "logs": "docker logs -f --tail=1000 api-container",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --detectOpenHandles --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "mockdate": "^3.0.5",
    "rimraf": "^5.0.1",
    "sucrase": "^3.33.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@netlify/functions": "^1.6.0",
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "fast-glob": "^3.3.0",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "mongodb-memory-server": "^8.13.0",
    "mongodb-memory-server-core": "^8.13.0",
    "mongodb-memory-server-global": "^8.13.0",
    "nodemon": "^3.0.1",
    "serverless-http": "^3.2.0",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
